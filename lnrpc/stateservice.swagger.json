        }
      }
    },
    "lnrpcWalletState": {
      "type": "string",
      "enum": [
        "NON_EXISTING",
        "LOCKED",
        "UNLOCKED",
        "RPC_ACTIVE",
        "WAITING_TO_START"
      ],
      "default": "NON_EXISTING"
    },
